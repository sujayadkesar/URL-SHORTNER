<!-- URL SHORTENING SECTION -->

<section class="page-section cta" id="urlshortner">
    <div class="container border-0">
        <div class="row mt-5">
            <div class="col-xl-9 mx-auto">
                <div class="text-center cta-inner rounded" id="borderthis">
                    <h2 class="section-heading mb-4"><span class="section-heading-upper">URL</span><span class="section-heading-lower"> SHORTNER</span></h2>


                    <div class="container">
                    
                        <form action="/shortUrls" method="POST" class="my-4 form-inline">
                          <label for="fullUrl" class="sr-only">Url</label>
                          <input required placeholder="Url" type="url" name="fullUrl" id="fullUrl" class="form-control col mr-2">
                          <button class="btn btn-success mt-5" type="submit">Shrink</button>
                        </form>
                        <table class="table table-striped table-responsive">
                          <thead>
                            <tr>
                              <th>Short URL</th>
                              <th>Copy-To-Clipboard</th>
                            </tr>
                          </thead>
                          <tbody>
                            <% shortUrls.forEach(shortUrl => { %>
                              <tr>
                                <td><a href="<%= shortUrl.short %>" id="copytext"><%= shortUrl.short %></a></td>
                                <td><button class="btn btn-warning" id="copyclipboard" onclick="myFunction()" onmouseout="ourFunc()" type="button">click-here-to-Copy</button></td>
                              </tr>
                            <% }) %>
                          </tbody>
                        </table>
                      </div>

